{
    "extends": ["../../.eslintrc.json"],
    "ignorePatterns": ["!**/*"],
    "overrides": [
      {
        "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
        "parserOptions": {
            "project": ["packages/docs/tsconfig(.*)?.json"],
            "ecmaFeatures": {
              "jsx": true
            }
        },
        "rules": {}
      },
      {
        "files": ["*.ts", "*.tsx"],
        "rules": {}
      },
      {
        "files": ["*.js", "*.jsx"],
        "rules": {}
      }
    ]
  }
  