  "keywords": [
    "radio"
  ],
  "author": "Junior Garcia <jrgarciadev@gmail.com>",
  "homepage": "https://heroui.com",
  "license": "MIT",
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heroui-inc/heroui.git",
    "directory": "packages/components/radio"
  },
  "bugs": {
    "url": "https://github.com/heroui-inc/heroui/issues"
